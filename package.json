    "@effect/schema": "0.35.0"
  },
  "devDependencies": {
    "@types/node": "18.17.12",
    "typescript": "5.2.2",
    "prettier": "3.0.3",
    "vitest": "0.34.3",
    "esbuild": "0.19.2"
  },
  "keywords": []
}
