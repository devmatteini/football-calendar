{
  "name": "football-calendar",
  "version": "1.0.0",
  "description": "Automatically sync your google calendar with football matches of your favorite team!",
  "author": "Cosimo Matteini",
  "license": "MIT",
  "scripts": {
    "format": "prettier -w src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:w": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:w": "vitest watch",
    "clean": "rm -rf .build",
    "build": "npm run clean && node esbuild.config.js",
    "start": "npm run build && node .build/index.js",
    "scripts:build": "esbuild --platform=node --bundle scripts/*.ts --outdir=scripts/.build/ --minify",
    "scripts:delete-events": "node scripts/.build/delete-events.js",
    "scripts:list-events": "node scripts/.build/list-events.js"
  },
  "dependencies": {
    "effect": "2.0.0-next.47",
    "@effect/schema": "0.39.2",
    "axios": "1.5.1",
    "@googleapis/calendar": "9.3.1",
    "commander": "11.0.0",
    "colorette": "2.0.20"
  },
  "devDependencies": {
    "@types/node": "18.17.12",
    "typescript": "5.2.2",
    "prettier": "3.0.3",
    "vitest": "0.34.6",
    "esbuild": "0.19.4"
  },
  "keywords": []
}
