{
  "name": "football-calendar",
  "version": "1.0.0",
  "description": "Automatically sync your google calendar with football matches of your favorite team!",
  "author": "Cosimo Matteini",
  "license": "MIT",
  "scripts": {
    "format": "prettier -w src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:w": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:w": "vitest watch",
    "clean": "rm -rf .build",
    "build": "npm run clean && node esbuild.config.js",
    "start": "npm run build && node .build/index.js",
    "scripts:build": "esbuild --platform=node --bundle scripts/*.ts --outdir=scripts/.build/ --minify",
    "scripts:delete-events": "node scripts/.build/delete-events.js"
  },
  "dependencies": {
    "@effect/io": "0.40.0",
    "@effect/data": "0.18.4",
    "@effect/schema": "0.35.0",
    "@effect/match": "0.35.2",
    "axios": "1.5.0",
    "googleapis": "126.0.1"
  },
  "devDependencies": {
    "@types/node": "18.17.12",
    "typescript": "5.2.2",
    "prettier": "3.0.3",
    "vitest": "0.34.3",
    "esbuild": "0.19.2"
  },
  "keywords": []
}
