{
  "name": "football-calendar",
  "version": "1.0.0",
  "description": "Add football matches to your calendar",
  "author": "Cosimo Matteini",
  "license": "TODO",
  "scripts": {
    "format": "prettier -w src/**/*.ts",
    "typecheck": "tsc --noEmit",
